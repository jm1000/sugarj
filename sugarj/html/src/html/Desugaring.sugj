package html;

import html.Syntax;
import org.sugarj.languages.Java;
import concretesyntax.Java;

public extension Desugaring{

	desugarings
		html-to-java
		
	rules
		html-to-java :
		
			Head(HeadOpen, Title(CharData), HeadClose)
			->
			|[ new HTMLHead(~CharData, null) ]|
		
		html-to-java :
		
			Head(HeadOpen, Title(CharData), Content, HeadClose)
			->
			|[ new HTMLHead(~CharData, ~(<htmlHeadCont-to-java> Content)) ]|
	
}